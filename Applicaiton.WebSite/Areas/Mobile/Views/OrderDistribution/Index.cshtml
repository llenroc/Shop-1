<div ng-controller="app.mobile.orderDistribution.index as vm" ng-cloak class="ng-cloak">
    <div class="blank" ng-if="!vm.orderDistributions||!vm.orderDistributions.length">@L("ThereIsNoItem")</div>
	<ul class="rebate-list" id="orderDistributionList">
		<li ng-repeat="orderDistribution in vm.orderDistributions">
            <div class="profileItem">
                <div class="profileItemAvatarBox">
                    <img width="48" height="48" class="profileTtemAvatar imageCircle" ng-src="{{orderDistribution.order.user.avatar}}">
                </div>
                <div class="profileItemBody">
                    <h6 class="profileItemName thin">{{orderDistribution.order.user.nickName}}</h6>
                </div>
            </div>
            <ul class="well-info-list">
                <li>
                    <label class="key">@L("DistributionMoney")</label>
                    <label class="value money" ng-bind="orderDistribution.money"></label>
                </li>
                <li>
                    <label class="key">@L("PayMoney")</label>
                    <label class="value money" ng-bind="orderDistribution.order.payMoney"></label>
                </li>
                <li>
                    <label class="key">@L("OrderTitle")</label>
                    <label class="value" ng-bind="orderDistribution.order.title"></label>
                </li>
                <li>
                    <label class="key">@L("CreationTime")</label>
                    <label class="value" ng-bind="orderDistribution.creationTime| momentFormat:'YYYY-MM-DD HH:mm:ss'"></label>
                </li>
            </ul>
		</li>
	</ul>
</div>
